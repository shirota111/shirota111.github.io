class Masutake {
  constructor(man, power) {
    this.man = man;
    this.power = power;
                          }
  stop() {
   
    $(this.man).prop('disabled', true); 
//manの無効化
    $(this.power).prop('disabled', true); 
//powerの無効化   }
                }


let masutake = new Masutake
('#manElement', '#powerElement');//各要素の無効化
masutake.stop(); 